{
  "files.exclude": {
    "yarn.lock": true,
    "node_modules": true,
    ".eslintignore": true,
    ".prettierrc.json": true,
    ".lintstagedrc.json": true,
    ".husky": true,
    "tsconfig.*": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "eslint.source.fixAll": true
  },
  "files.autoSave": "off"
}
